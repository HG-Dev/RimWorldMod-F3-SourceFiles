<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="FFFApparelNoQualityBase">
    <defName>FFF_Hypermesh_Suit</defName>
    <label>hypermesh protosuit</label>
    <description>A machine-printed suit template. Although a perfectly reasonable and hygenic choice for low-ranking ship crew or research staff, it lacks equipment required for space walks, a surface layer to hide its raw hypermesh, or pockets. The shapeshifting qualities of hypermesh make it form-fitting... awkwardly so.</description>
    <generateCommonality>0.1</generateCommonality>
    <graphicData> 
      <texPath>Things/Pawn/Humanlike/Apparel/Hypermesh/Hypermesh</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
      <Mass>1.71</Mass>
      <WorkToMake>5000</WorkToMake>
      <MaxHitPoints>300</MaxHitPoints>
      <ArmorRating_Blunt>0.25</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.33</ArmorRating_Sharp>
      <ArmorRating_Heat>0.25</ArmorRating_Heat>
      <Insulation_Cold>20</Insulation_Cold>
      <Insulation_Heat>20</Insulation_Heat>
      <EquipDelay>3</EquipDelay>
    </statBases>
    <thingSetMakerTags>
      <li>RewardStandardMidFreq</li>
    </thingSetMakerTags>
    <equippedStatOffsets>
      <CarryingCapacity>-2.5</CarryingCapacity>
      <SocialImpact>0.1</SocialImpact>
	    <ToxicSensitivity>-0.2</ToxicSensitivity>
      <MedicalSurgerySuccessChance>0.1</MedicalSurgerySuccessChance>
      <MedicalTendQuality>0.1</MedicalTendQuality>
      <ResearchSpeed>0.1</ResearchSpeed>
      <HygieneRateMultiplier MayRequire="dubwise.dubsbadhygiene">-0.5</HygieneRateMultiplier>
    </equippedStatOffsets>
    <costList>
      <Hyperweave>70</Hyperweave>
      <ComponentIndustrial>2</ComponentIndustrial>
    </costList>
    <apparel>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
        <li>Anything</li>
      </defaultOutfitTags>
      <tags>
        <li>SpacerMilitary</li>
        <li>Hypermesh</li>
      </tags>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/Hypermesh/Hypermesh</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <layers>
          <li>OnSkin</li>
      </layers>
      <bodyPartGroups>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Torso</li>
        <li>Arms</li>
        <li>Legs</li>
        <li>Hands</li>
				<li>Feet</li>
      </bodyPartGroups>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <only>(1,1,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>

