<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="FFFApparelMakeableBase">
    <defName>FFF_RegalFlak</defName>
    <label>dress flak vest</label>
    <description>A precision-engineered bulletproof vest of plasteel and hyperweave provided to spacer military officers. Boosts life support capabilities during extravehicular activity.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/RegalFlak/RegalFlak</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <thingCategories Inherit='False'>
      <li>ApparelArmor</li>
    </thingCategories>
	<tradeTags>
      <li>ExoticMisc</li>
      <li>Armor</li>
    </tradeTags>
    <recipeMaker>
      <researchPrerequisite>FlakArmor</researchPrerequisite>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
      <recipeUsers>
        <li>TableMachining</li>
      </recipeUsers>
    </recipeMaker>
    <stuffCategories>
        <li>Fabric</li>
        <li>Leathery</li>
    </stuffCategories>
    <costStuffCount>20</costStuffCount>
    <costList>
      <ComponentIndustrial>4</ComponentIndustrial>
      <Plasteel>40</Plasteel>
      <Hyperweave>15</Hyperweave>
      <Gold>1</Gold>
    </costList>
    <thingSetMakerTags>
      <li>RewardStandardMidFreq</li>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <statBases>
      <WorkToMake>10000</WorkToMake>
      <MaxHitPoints>450</MaxHitPoints>
      <Mass>6</Mass>
      <Flammability>0.2</Flammability>
      <ArmorRating_Blunt>0.63</ArmorRating_Blunt>
      <ArmorRating_Sharp>1.15</ArmorRating_Sharp>
      <ArmorRating_Heat>0.4</ArmorRating_Heat>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <Insulation_Cold>33</Insulation_Cold>
      <Insulation_Heat>5</Insulation_Heat>
      <EquipDelay>8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <CarryingCapacity>2.5</CarryingCapacity>
      <SocialImpact>0.12</SocialImpact>
      <MoveSpeed>-0.12</MoveSpeed>
      <JumpRange MayRequire="Ludeon.RimWorld.Royalty">3</JumpRange>
	  <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">0.1</VacuumResistance>
	  <VacuumSpeedMultiplier>3</VacuumSpeedMultiplier>
    </equippedStatOffsets>
    <generateCommonality>1</generateCommonality>
    <apparel>
      <bodyPartGroups>
        <li>Neck</li>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Waist</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/RegalFlak/RegalFlak</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>SpacerMilitary</li>
        <li>RoyalTier5</li>
        <li>PrestigeCombatGear</li>
      </tags>
	  <canBeDesiredForIdeo>true</canBeDesiredForIdeo>
      <defaultOutfitTags>
        <li>Anything</li>
        <li>Soldier</li>
        <li>Knight</li>
        <li>Diplomat</li>
      </defaultOutfitTags>
      <useDeflectMetalEffect>true</useDeflectMetalEffect>
    </apparel>
    <colorGenerator Class="ColorGenerator_White">
    </colorGenerator>
  </ThingDef>

	<ThingDef ParentName="ArmorMachineableBase">
		<defName>Apparel_FFF_HeavyEvaSuitChild</defName>
		<label>kid heavy EVA suit</label>
		<description>Although unrelated to hypermesh technology, F3 research data includes design specifications for a child-sized EVA suit that, alarmingly, implies potential combat situations. Includes embedded thrusters for movement.</description>
		<techLevel>Spacer</techLevel>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<researchPrerequisite>ShipSpaceSuits</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>FabricationBench</li>
			</recipeUsers>
		</recipeMaker>
		<graphicData>
			<texPath>Things/Pawn/Humanlike/Apparel/ChildEva/FFFChildEvaSuitItem</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<WorkToMake>17000</WorkToMake>
			<MaxHitPoints>340</MaxHitPoints>
			<Mass>10</Mass>
			<ArmorRating_Blunt>0.45</ArmorRating_Blunt>
			<ArmorRating_Sharp>1.08</ArmorRating_Sharp>
			<ArmorRating_Heat>0.75</ArmorRating_Heat>
			<Insulation_Cold>100</Insulation_Cold>
			<Insulation_Heat>25</Insulation_Heat>
        <EquipDelay>8</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.25</MoveSpeed>
			<WorkSpeedGlobal>-0.075</WorkSpeedGlobal>
			<VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">0.3</VacuumResistance>
			<!--<DecompressionResistance>0.51</DecompressionResistance>
			<VacuumSpeedMultiplier>4</VacuumSpeedMultiplier>-->
		</equippedStatOffsets>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<tradeTags>
			<li>HiTechArmor</li>
   		</tradeTags>
		<costList>
			<ComponentSpacer>1</ComponentSpacer>
			<Plasteel>20</Plasteel>
      		<Steel>10</Steel>
		</costList>
		<generateCommonality>2</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
				<li>Hands</li>
				<li>Feet</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Humanlike/Apparel/ChildEva/FFFChildEvaSuit</wornGraphicPath>
			<canBeDesiredForIdeo>false</canBeDesiredForIdeo>
			<developmentalStageFilter>Child</developmentalStageFilter>
			<shellRenderedBehindHead>true</shellRenderedBehindHead>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>Spacer</li>
				<li>EVA</li>
			</tags>
			<defaultOutfitTags />
		</apparel>
	</ThingDef>

  	<ThingDef ParentName="HatMakeableBase">
		<defName>Apparel_FFF_HeavyEvaHelmetChild</defName>
		<label>kid heavy EVA helmet</label>
		<description>The helmet counterpart to the 'heavy' EVA suit for children found in F3 files. The specifications state that the 'animal PLSS covering is important to comfort children in hazardous conditions.' Recycles oxygen and provides resistance to toxic airborne particulates.</description>
		<techLevel>Spacer</techLevel>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<researchPrerequisite>ShipSpaceSuits</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>FabricationBench</li>
			</recipeUsers>
		</recipeMaker>
		<graphicData>
			<texPath>Things/Pawn/Humanlike/Apparel/ChildEva/FFFChildEvaHelmetItem</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<statBases>
			<WorkToMake>12000</WorkToMake>
			<MaxHitPoints>160</MaxHitPoints>
			<Mass>2.7</Mass>
			<ArmorRating_Sharp>1.06</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.45</ArmorRating_Blunt>
			<ArmorRating_Heat>0.75</ArmorRating_Heat>
			<Insulation_Cold>50</Insulation_Cold>
			<Insulation_Heat>25</Insulation_Heat>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.04</MoveSpeed>
			<ToxicResistance>0.5</ToxicResistance>
			<ToxicEnvironmentResistance>1.0</ToxicEnvironmentResistance>
			<VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">0.68</VacuumResistance>
			<!--<HypoxiaResistance>1</HypoxiaResistance>
			<DecompressionResistance>0.5</DecompressionResistance>-->
		</equippedStatOffsets>
		<thingCategories>
			<li>ArmorHeadgear</li>
		</thingCategories>
		<tradeTags>
			<li>HiTechArmor</li>
   		</tradeTags>
    	<stuffCategories>
			<li>Fabric</li>
		</stuffCategories>
		<costStuffCount>15</costStuffCount>
		<costList>
			<ComponentSpacer>1</ComponentSpacer>
			<Plasteel>10</Plasteel>
		</costList>
		<generateCommonality>2</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>FullHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Humanlike/Apparel/ChildEva/FFFChildEvaHelmet</wornGraphicPath>
			<canBeDesiredForIdeo>false</canBeDesiredForIdeo>
			<developmentalStageFilter>Child</developmentalStageFilter>
			<useWornGraphicMask>true</useWornGraphicMask>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>Spacer</li>
				<li>EVA</li>
			</tags>
			<defaultOutfitTags/>
		</apparel>
	</ThingDef>

</Defs>