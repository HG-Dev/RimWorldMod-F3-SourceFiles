<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>AutoMake_Protosuit</defName>
    <label>print protosuit</label>
    <description>Automatically print a hypermesh protosuit.</description>
    <jobString>Printing hypermesh protosuit.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <requiredGiverWorkType>Hauling</requiredGiverWorkType>
    <effectWorking>HackingTerminal</effectWorking>
    <soundWorking>Interact_Research</soundWorking>
    <workAmount>24</workAmount>
    <formingTicks>8000</formingTicks>
    <descriptionHyperlinks>
      <ThingDef>FFF_Hypermesh_Suit</ThingDef>
    </descriptionHyperlinks>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Hyperweave</li>
          </thingDefs>
        </filter>
        <count>70</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Hyperweave</li>
        <li>ComponentIndustrial</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <FFF_Hypermesh_Suit>1</FFF_Hypermesh_Suit>
    </products>
    <displayPriority>230</displayPriority>
  </RecipeDef>

